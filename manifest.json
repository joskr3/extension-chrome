{
  "manifest_version":3,
  "name":"Mi extension",
  "version":"1.0.0",
  "description": "Una descripcion sencilla",
  "action": {
    "default_popup":"popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker":"background.js"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["content.js"]
    }
  ]
}
